@model IEnumerable<ZJV.DVDCentral.BL.Models.Genre>

    <table>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.ActionLink(item.Description, "Browse", "Movie", new { id = item.Id }, null)
                </td>
            </tr>
        }
        <tr>
            <td>@Html.ActionLink("Show All", "Index", "Movie")</td>
        </tr>
        <tr>
            <td>@Html.ActionLink("Login", "Login", "User")</td>
        </tr>
        <tr>
            <td>@Html.ActionLink("Logout", "Logout", "User")</td>
        </tr>
    </table>

